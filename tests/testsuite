$at_traceon


      $at_traceoff
      $at_times_p && times >"$at_times_file"
    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat "$at_status_file"`
    ;;

  3 ) # 3. lbref.at:17: Resolving local backward references
    at_setup_line='lbref.at:17'
    at_desc="Resolving local backward references"
    $at_quiet $ECHO_N "  3: Resolving local backward references            $ECHO_C"
